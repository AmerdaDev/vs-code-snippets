{
	// Place your snippets for html here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	"Foreach Loop": {
		"prefix": "foreach",
		"body": [
			"<?php foreach($1 as $2): ?>"
		],
		"description": "Foreach"
	},
	"For Loop": {
		"prefix": "for",
		"body": [
			"<?php for($1 = 0; $1 < $2 ; $1++): ?>"
		],
		"description": "For"
	},
	"End Foreach Loop": {
		"prefix": "endforeach",
		"body": [
			"<?php endforeach; ?>"
		],
		"description": "End Foreach"
	},
	"End For Loop": {
		"prefix": "endfor",
		"body": [
			"<?php endfor; ?>"
		],
		"description": "End For"
	},
	"if Statement": {
		"prefix": "if",
		"body": [
			"<?php if($1): ?>"
		],
		"description": "if Statement"
	},
	"if get_field": {
		"prefix": "ifget",
		"body": [
			"<?php if(get_field('$1')): ?>"
		],
		"description": "if get_field Statement"
	},
	"if have_rows": {
		"prefix": "ifhr",
		"body": [
			"<?php if(have_rows('$1')): ?>"
		],
		"description": "if have_rows Statement"
	},
	"while have_rows": {
		"prefix": "whr",
		"body": [
			"<?php while(have_rows('$1')): ?>"
		],
		"description": "while have_rows Statement"
	},
	"if get_sub_field": {
		"prefix": "ifgets",
		"body": [
			"<?php if(get_sub_field('$1')): ?>"
		],
		"description": "if get_field Statement"
	},
	"if get_field option": {
		"prefix": "ifgeto",
		"body": [
			"<?php if(get_field('$1', 'option')): ?>"
		],
		"description": "if get_field('fieldname', option) Statement"
	},
	"get_field('fieldname')": {
		"prefix": "getf",
		"body": [
			"<?= get_field('$1') ?>"
		],
		"description": ""
	},
	"get_sub_field('fieldname')": {
		"prefix": "getsf",
		"body": [
			"<?= get_sub_field('$1') ?>"
		],
		"description": ""
	},
	"get_field('fieldname', 'option')": {
		"prefix": "getfo",
		"body": [
			"<?= get_field('$1', 'option') ?>"
		],
		"description": ""
	},
	"php Tag": {
		"prefix": "php",
		"body": [
			"<?php $1 ?>"
		],
		"description": "Php tag"
	},
	"endif Statement": {
		"prefix": "endif",
		"body": [
			"<?php endif; ?>"
		],
		"description": "endif Statement"
	},
	"endwhile loop": {
		"prefix": "endwhile",
		"body": [
			"<?php endwhile; ?>"
		],
		"description": "endwhile loop"
	},
	"elseif Statement": {
		"prefix": "elseif",
		"body": [
			"<?php else if($1): ?>"
		],
		"description": "elseif Statement"
	},
	"_e": {
		"prefix": "_e",
		"body": [
			"<?php _e('$1', '$2'); ?>"
		],
		"description": "_e translate function"
	},
	"else Statement": {
		"prefix": "else",
		"body": [
			"<?php else: ?>"
		],
		"description": "else Statement"
	},
	"Var Dump": {
		"prefix": "var_dump",
		"body": [
			"<?php var_dump($1); ?>"
		],
		"description": "Var Dump"
	},
